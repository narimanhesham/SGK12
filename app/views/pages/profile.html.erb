<% provide(:profile, "active") %>
<% provide(:title, "Profile") %>

<div class="container bot-padding">
  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
      <div class="pull-left profile-title">
        <div>
          <h1>Hello, Nariman</h1>
        </div>
        <div class="profile-content">
          <h4>I'm feeling great today and I'm so strong.</h4>
          <h4>You're doing great so keep it up.</h4>
          <h4>And don't forget to check on my health later on.</h4>
        </div>
      </div>
      <div class="pull-right profile-img">
        <%= image_tag('girlSmurf.png') %>
      </div> 
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 profile-title">
      <h3>My Health:</h3>
      <div class="progress health-bar">
        <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
          <span class="sr-only">100% Complete (danger)</span>
        </div>
      </div>
    </div>
  </div>
</div>
